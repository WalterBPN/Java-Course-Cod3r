package fundamentos;

public class DesafioTemperatura {
	
	public static void main(String[] Args) {
		
		//Converting Fahrenheit to Celsius;
		
		double fahrenheit = 50.0;
		final double fahToCelsius = (fahrenheit - 32) * (5/9);
		
		System.out.println(fahrenheit + " graus Fahrenheit Ã© equivalente a " + fahToCelsius + " graus Celsius.");
		
		
		
		//Converting Celsius to Fahrenheit;
		
		double celsius = 30.0;
		
	}

}
